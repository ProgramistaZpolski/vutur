{
    "env": {
        "node": true,
        "es2021": true
    },
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "script"
    },
    "rules": {
		"for-direction": 2,
        "eqeqeq": 2,
        "getter-return": 2,
        "no-async-promise-executor": 2,
        "no-cond-assign": 2,
        "no-constant-condition": 2,
        "no-control-regex": 2,
        "no-dupe-args": 2,
        "no-dupe-else-if": 2,
        "no-dupe-keys": 2,
        "no-duplicate-case": 2,
        "no-duplicate-imports": 2,
        "no-empty": 2,
        "no-empty-character-class": 2,
        "no-ex-assign": 2,
        "no-extra-boolean-cast": 2,
        "no-extra-parens": 2,
        "no-func-assign": 2,
        "no-import-assign": 2,
        "no-invalid-regexp": 2,
        "no-irregular-whitespace": 2,
        "no-loss-of-precision": 2,
        "no-misleading-character-class": 2,
        "no-obj-calls": 2,
        "no-promise-executor-return": 2,
        "no-regex-spaces": 2,
        "no-setter-return": 2,
        "no-sparse-arrays": 2,
        "no-template-curly-in-string": 2,
        "no-unreachable": 2,
        "no-unreachable-loop": 2,
        "no-unsafe-negation": 2,
        "no-unsafe-finally": 2,
        "no-unsafe-optional-chaining": 2,
        "no-useless-backreference": 2,
        "require-atomic-updates": 2,
        "use-isnan": 2,
        "valid-typeof": 2,
        "accessor-pairs": 2,
        "array-callback-return": 2,
        "block-scoped-var": 2,
        "curly": 2,
        "default-case": 2,
        "default-case-last": 2,
        "default-param-last": 2,
        "dot-notation": 2,
        "grouped-accessor-pairs": 2,
        "guard-for-in": 2,
        "no-case-declarations": 2,
        "no-constructor-return": 2,
        "no-div-regex": 2,
        "no-empty-function": 2,
        "no-eq-null": 2,
        "no-eval": 2,
        "no-var": 2,
        "no-extend-native": 2,
        "no-extra-label": 2,
        "no-extra-bind": 2,
        "no-fallthrough": 2,
        "no-floating-decimal": 2,
        "no-global-assign": 2,
        "no-invalid-this": 2,
        "no-iterator": 2,
        "no-lone-blocks": 2,
        "no-loop-func": 2,
        "no-multi-spaces": 2,
        "no-new": 2,
        "no-new-wrappers": 2,
        "no-nonoctal-decimal-escape": 2,
        "no-octal": 2,
        "no-redeclare": 2,
        "no-return-assign": 2,
        "no-return-await": 2,
        "no-self-assign": 2,
        "no-self-compare": 2,
        "no-throw-literal": 2,
        "no-unused-labels": 2,
        "no-useless-call": 2,
        "no-useless-catch": 2,
        "no-useless-concat": 2,
        "no-useless-escape": 2,
        "no-with": 2,
        "yoda": 2,
        "no-delete-var": 2,
        "no-label-var": 2,
        "no-shadow": 2,
        "no-undef-init": 2,
        "no-use-before-define": 2,
        "indent": [
            "error",
            "tab"
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "double"
        ],
        "semi": [
            "error",
            "always"
        ]
    }
}
